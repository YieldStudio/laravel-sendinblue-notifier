{
    "name": "yieldstudio/laravel-sendinblue-notifier",
    "description": "Easily send Sendinblue transactional email and sms with Laravel notifier.",
    "type": "plugin",
    "keywords": [
        "yieldstudio",
        "laravel",
        "notifier",
        "notification",
        "sendinblue",
        "email",
        "sms"
    ],
    "homepage": "https://github.com/YieldStudio/laravel-sendinblue-notifier",
    "license": "MIT",
    "authors": [
        {
            "name": "James Hemery",
            "email": "james@yieldstudio.fr",
            "homepage": "https://yieldstudio.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9",
        "illuminate/support": "^9",
        "sendinblue/api-v3-sdk": "^8.3"
    },
    "require-dev": {
        "ciareis/bypass": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "orchestra/testbench": "7.*",
        "pestphp/pest": "^1.21",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "YieldStudio\\LaravelSendinblueNotifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YieldStudio\\LaravelSendinblueNotifier\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/pest",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
              "SendinblueNotifierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
